<header class="main-header">
    <nav class="navbar navbar-static-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <%if(gUserId.length == 0){%>
                    <a href="<%= gRoot %>/login" class="navbar-brand"><b>CMS</b></a>
                <%}else {%>
                    <a href="<%= gRoot %>/customer" class="navbar-brand"><b>CMS</b></a>
                <%}%>
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                        data-target="#navbar-collapse">
                    <i class="fa fa-bars"></i>
                </button>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav">
                    <!--0: admin, 1:店舗, 2:旅行会社-->
                    <%if(gUserStatus == 0){%>
                    <li class="dropdown">
                        <a href="<%= gRoot %>/customer" class="dropdown-toggle" data-toggle="dropdown">
                            <script>document.write(CUSTOMER_USER_MANAGER)</script>
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="<%= gRoot %>/customer/user/register"> <script>document.write(CUSTOMER_USER_REGISTER);</script></a></li>
                            <li><a href="<%= gRoot %>/customer/user/list"> <script>document.write(CUSTOMER_USER_LIST); </script></a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="<%= gRoot %>/customer" class="dropdown-toggle" data-toggle="dropdown">
                            <script>document.write(CUSTOMER_DATA_MANAGER)</script>
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="<%= gRoot %>/customer/data/list"><script>document.write(CUSTOMER_DATA_LIST); </script></a></li>
                        </ul>
                    </li>
                    <%}else if(gUserStatus == 1){%>
                    <li class="dropdown">
                        <a href="<%= gRoot %>/customer" class="dropdown-toggle" data-toggle="dropdown">
                            <script>document.write(CUSTOMER_DATA_MANAGER)</script>
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="<%= gRoot %>/customer/data/register"><script>document.write(CUSTOMER_DATA_REGISTER); </script></a></li>
                            <li><a href="<%= gRoot %>/customer/data/list"><script>document.write(CUSTOMER_DATA_LIST);</script></a></li>
                        </ul>
                    </li>
                    <%}else if(gUserStatus == 2){%>
                    <li class="dropdown">
                        <a href="<%= gRoot %>/customer" class="dropdown-toggle" data-toggle="dropdown">
                            <script>document.write(CUSTOMER_DATA_MANAGER)</script>
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="<%= gRoot %>/customer/data/list"><script>document.write(CUSTOMER_DATA_LIST);</script></a></li>
                        </ul>
                    </li>
                    <%}else {%>

                    <%}%>
                </ul>

                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="<%= gRoot %>/logout">
                            <span id="header_user_id" class="hidden-xs" style="margin-right:10px;"><%= gUserId %></span>
                            <i class="glyphicon glyphicon-off"></i>
                        </a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>
</header>