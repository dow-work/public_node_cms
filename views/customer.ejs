<!DOCTYPE html>
<html>
<head>
	<%include ./partials/include%>
	<%if(gViewName == 'data_register'){%>
		<script src="<%=gRoot%>/libraries/js-xlsx/shim.min.js" type="text/javascript"></script>
		<script src="<%=gRoot%>/libraries/js-xlsx/xlsx.full.min.js" type="text/javascript"></script>
	<%}%>


	<%if(gViewName != 'customer'){%><script src="<%=gRoot%>/javascripts/<%=gViewName%>.js" type="text/javascript"></script><%}%>
	<title><%=global.gTitle%></title>
</head>

<body class="skin-blue layout-top-nav">
	<!--<div class="wrapper">-->
		<%include ./partials/header%>
		<div class="content-wrapper">
			<section class="content-header">
				<h1><%=gMenu%></h1>
				<ol class="breadcrumb">
					<li class="gray"><%=gTree%></li>
					<li><%=gMenu%></li>
				</ol>
			</section>			
			<section class="content" style="background: #FFFFFF">
				<%if(bindingData == 0){%>
					<%=bindingData%>
				<%}else{%>
					<%-bindingData%>
				<%}%>
 			</section>
			<input type="hidden" id="page_param" value="<%=bindingParam%>" />
			<input type="hidden" id="paid_param" value="<%=gCompany%>" />
			<input type="hidden" id="store_param" value="<%=gStore%>" />
			<input type="hidden" id="status_param" value="<%=gUserStatus%>" />
		</div>
	<!--</div>-->

	<div class="wrap-loading display-none">
		<div class="overlay">
			<i class="fa fa-refresh fa-spin"></i>
		</div>
	</div>
	<%if(gViewName == 'data_list'){%>
	<div class="modal modal-danger fade" id="modal-danger">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span></button>
					<h4 class="modal-title"><script>document.write(ONE_DATE_DB_DELETE_BTN);</script></h4>
				</div>
				<div class="modal-body">
						<ul id="modal_list_ul" name="modal_list_ul">
						<li name="modal_list_li">Date : </li>
						<li name="modal_list_li"><input type="text" id="modalDateId" name="dateone" value="2018-01-01" /></li>
						<li name="modal_list_li">Store: <input type="text" name="modalStore" id="modalStoreId"/></li>
						<!--<li name="modal_list_li">-->
							<!--<span class="btn btn-default btn-flat" name="btn_modal_search">Search</span>-->
					   <!--</li>-->
					</ul>

					<p id="p_description"><script>document.write(ONE_DAY_DELETE_MSG);</script></p>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-outline pull-left" data-dismiss="modal" ><script>document.write(CLOSE_BTN);</script></button>
					<button type="button" class="btn btn-outline" id="modal_action_btn"><script>document.write(ONE_DATE_DB_DELETE_BTN);</script></button>
				</div>
			</div>
			<!-- /.modal-content -->
		</div>
		<!-- /.modal-dialog -->
	</div>
	<!-- /.modal -->
	<div class="modal modal-danger fade" id="modal-danger-all">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span></button>
					<h4 class="modal-title"><script>document.write(DB_TRUNCATE_BTN);</script></h4>
				</div>
				<div class="modal-body">
					<p id="p_description"><script>document.write(WARRING_TRUNCATE_MSG);</script></p>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-outline pull-left" data-dismiss="modal"><script>document.write(CLOSE_BTN);</script></button>
					<button type="button" class="btn btn-outline" id="all_modal_action_btn"><script>document.write(DB_TRUNCATE_BTN);</script></button>
				</div>
			</div>
			<!-- /.modal-content -->
		</div>
		<!-- /.modal-dialog -->
	</div>
	<!-- /.modal -->
	<%}%>
</body>
<%if(gViewName == 'data_list'){%>
<link href="<%=gRoot%>/stylesheets/data_list.css" rel="stylesheet" type="text/css" />
<%}%>
</html>